{
    "rules": {
        "cx-member-ordering": [
            true,
            {
                "alphabetize": true,
                "order": [
                    "private-decorated-field",
                    "protected-decorated-field",
                    "public-decorated-field",
                    "private-abstract-field",
                    "private-readonly-field",
                    "private-static-field",
                    "private-instance-field",
                    "protected-abstract-field",
                    "protected-readonly-field",
                    "protected-static-field",
                    "protected-instance-field",
                    "public-abstract-field",
                    "public-readonly-field",
                    "public-static-field",
                    "public-instance-field",
                    "public-decorated-getter",
                    "private-decorated-getter",
                    "private-decorated-setter",
                    "private-decorated-method",
                    "protected-decorated-getter",
                    "protected-decorated-setter",
                    "protected-decorated-method",
                    "public-decorated-setter",
                    "public-decorated-method",
                    "public-abstract-getter",
                    "public-abstract-setter",
                    "public-abstract-method",
                    "public-instance-getter",
                    "private-abstract-getter",
                    "private-abstract-setter",
                    "private-abstract-method",
                    "private-instance-getter",
                    "private-instance-setter",
                    "private-static-method",
                    "private-static-getter",
                    "private-static-setter",
                    "private-instance-method",
                    "protected-abstract-getter",
                    "protected-abstract-setter",
                    "protected-abstract-method",
                    "protected-instance-getter",
                    "protected-instance-setter",
                    "protected-static-method",
                    "protected-static-getter",
                    "protected-static-setter",
                    "protected-instance-method",
                    "public-instance-setter",
                    "public-static-method",
                    "public-static-getter",
                    "public-static-setter",
                    "constructor",
                    "ng-instance-method",
                    "public-instance-method"
                ]
            }
        ]
    }
}
